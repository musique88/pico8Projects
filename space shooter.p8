--utilz

function collision(o1,o2)
	if (o1.y >= o2.y-(o2.h/2) and
					o1.y <= (o2.y+(o2.h/2)) and
					o1.x >= o2.x-(o2.w/2) and
					o1.x <= (o2.x+(o2.w/2))) then
		return true
	else
		return false
	end
end

function distance(o1,o2)
	a = abs((o1.x+o1.w/2) - (o2.x+o2.w/2))
	b = abs((o1.y+o1.h/2) - (o2.y+o2.h/2))
	return sqrt(a^2+b^2)
end
